function get_tree(){var e='<div class="cell border-box-sizing text_cell rendered"><div class="prompt input_prompt"></div><div class="inner_cell"><div class="text_cell_render border-box-sizing rendered_html"><p><strong>文章汇总：<a href="https://www.cnblogs.com/dotnetcrazy/p/9160514.html" target="_blank">https://www.cnblogs.com/dotnetcrazy/p/9160514.html</a></strong></p>{dntnav}</div></div></div>',t=$("#cnblogs_post_body h2,#cnblogs_post_body h3,#cnblogs_post_body h4");if(t.length>0){var o='<div id="navCategory">';o+="<p><b>目录：</b></p>",o+="<ul>";for(var i=0;i<t.length;i++){var a='<a name="_map'+i+'"></a>';$(t[i]).before(a);var l=t[i].tagName.toLowerCase(),n="";"h2"==l?n='<li><a href="#_map'+i+'">'+$(t[i]).text()+"</a></li>":"h3"==l?n='<li style="padding-left: 0.5em;"><a href="#_map'+i+'">'+$(t[i]).text()+"</a></li>":"h4"==l&&(n='<li style="padding-left: 1em;"><a href="#_map'+i+'">'+$(t[i]).text()+"</a></li>"),o+=n}o+="</ul>",o+="</div>",0!=$("#cnblogs_post_body").length&&$($("#cnblogs_post_body")[0]).prepend(e.replace("{dntnav}",o))}}function detail_img(){0==$("#show_box").size()&&$("body").append('<div id="show_box"><img src="" /></div>'),$("img").on("click",function(e){if($imgurl=$(this).attr("src"),$imgurl){var t=$("#show_box");t.find("img").attr("src",$imgurl),easyDialog.open({container:"show_box",autoClose:3e4,fixed:!1})}}),$(document).on("click","#show_box",function(){$(this).fadeOut(200),easyDialog.close()}),$(document).on("click","#overlay",function(){$(this).fadeOut(200),easyDialog.close()})}